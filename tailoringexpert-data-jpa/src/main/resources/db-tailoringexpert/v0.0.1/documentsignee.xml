<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="1" author="Michael Baedorf">
        <createTable tableName="DOCUMENTSIGNEE">
            <column name="DOCUMENTSIGNEE_ID" type="BIGINT">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="FACULTY" type="VARCHAR(256)"/>
            <column name="SIGNEE" type="VARCHAR(256)"/>
            <column name="STATE" type="VARCHAR(64)"/>
            <column name="POSITION" type="BIGINT"/>
        </createTable>
    </changeSet>

</databaseChangeLog>
