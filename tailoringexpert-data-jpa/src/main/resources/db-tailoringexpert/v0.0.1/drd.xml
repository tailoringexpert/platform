<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="1" author="Michael Baedorf">
        <createTable tableName="DRD">
            <column name="DRD_ID" type="BIGINT">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="NUMBER" type="VARCHAR(128)"/>
            <column name="TITLE" type="VARCHAR(256)"/>
            <column name="DELIVERYDATE" type="VARCHAR(256)"/>
            <column name="ACTION" type="VARCHAR(64)"/>
        </createTable>
    </changeSet>

</databaseChangeLog>
