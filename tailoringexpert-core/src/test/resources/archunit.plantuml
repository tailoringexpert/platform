@startuml

skinparam componentStyle uml2
'skinparam component {
'  BorderColor #grey
'  BackgroundColor #white
'}

[Domain] <<..domain..>>
[Renderer] <<..renderer..>>
[Repository] <<..repository..>>

[Anforderung] <<..anforderung..>>
[Domain] <-- [Anforderung]
[Repository] <-- [Anforderung]

[ScreeningSheet] <<..screeningsheet..>>
[Domain] <-- [ScreeningSheet]
[Repository] <-- [ScreeningSheet]

[Tailoring] <<..tailoring..>>
[Domain] <-- [Tailoring]
[Repository] <-- [Tailoring]
[Renderer] <-- [Tailoring]
[ScreeningSheet] <-- [Tailoring]
[Anforderung] <-- [Tailoring]

[Projekt] <<..projekt..>>
[Domain] <-- [Projekt]
[Repository]  <- [Projekt]
[ScreeningSheet] <-- [Projekt]
[Tailoring] <-- [Projekt]

[Katalog] <<..katalog..>>
[Domain] <-- [Katalog]
[Repository] <-- [Katalog]
[Renderer] <-- [Katalog]

@enduml